  "calendar_aging_model": {
    "description": "Calendar aging in NMC is strongly influenced by voltage (SOC) and temperature. High SOC (>80%) accelerates degradation exponentially.",
    "equation": "Q_loss_cal = alpha_cal * t^beta_cal * exp(-E_a_cal / (R*T)) * SOC^gamma_cal",
    "parameters": {
      "alpha_cal": {
        "value": 0.035,
        "unit": "dimensionless",
        "description": "Calendar aging pre-exponential factor (higher than LFP)"
      },
      "beta_cal": {
        "value": 0.5,
        "unit": "dimensionless",
        "description": "Time dependency exponent (square root of time)"
      },
      "E_a_cal": {
        "value": 0.65,
        "unit": "eV",
        "description": "Calendar aging activation energy (higher temperature sensitivity than LFP)"
      },
      "gamma_cal": {
        "value": 0.5,
        "unit": "dimensionless",
        "description": "SOC dependency exponent (stronger SOC effect than LFP)"
      },
      "R": {
        "value": 8.617e-5,
        "unit": "eV/K",
        "description": "Boltzmann constant"
      }
    },
    "reference_conditions": {
      "temperature_C": 25,
      "SOC": 0.5,
      "capacity_fade_percent_per_year": 4.0
    }
  },
  "cycle_aging_model": {
    "description": "Cycle aging in NMC occurs through particle cracking and transition metal dissolution, accelerated by high DOD and temperature.",
    "equation": "Q_loss_cyc = alpha_cyc * N^beta_cyc * exp(-E_a_cyc / (R*T)) * DOD^gamma_cyc",
    "parameters": {
      "alpha_cyc": {
        "value": 0.025,
        "unit": "dimensionless",
        "description": "Cycle aging pre-exponential factor"
      },
      "beta_cyc": {
        "value": 0.55,
        "unit": "dimensionless",
        "description": "Cycle count dependency exponent"
      },
      "E_a_cyc": {
        "value": 0.45,
        "unit": "eV",
        "description": "Cycle aging activation energy"
      },
      "gamma_cyc": {
        "value": 1.0,
        "unit": "dimensionless",
        "description": "DOD dependency exponent (linear DOD effect)"
      }
    },
    "reference_conditions": {
      "temperature_C": 25,
      "DOD": 0.8,
      "C_rate": 0.5,
      "cycles_to_80_percent_SOH": 2000
    }
  },
