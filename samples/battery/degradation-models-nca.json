  "calendar_aging_model": {
    "description": "NCA calendar aging is driven by SEI growth and electrolyte decomposition. Better thermal stability than NMC but still sensitive to high SOC.",
    "equation": "Q_loss_cal = alpha_cal * t^beta_cal * exp(-E_a_cal / (R*T)) * SOC^gamma_cal",
    "parameters": {
      "alpha_cal": {
        "value": 0.038,
        "unit": "dimensionless",
        "description": "Calendar aging pre-exponential factor (highest among Li-ion chemistries)"
      },
      "beta_cal": {
        "value": 0.5,
        "unit": "dimensionless",
        "description": "Time dependency exponent (square root of time)"
      },
      "E_a_cal": {
        "value": 0.58,
        "unit": "eV",
        "description": "Calendar aging activation energy"
      },
      "gamma_cal": {
        "value": 0.45,
        "unit": "dimensionless",
        "description": "SOC dependency exponent"
      },
      "R": {
        "value": 8.617e-5,
        "unit": "eV/K",
        "description": "Boltzmann constant"
      }
    },
    "reference_conditions": {
      "temperature_C": 25,
      "SOC": 0.5,
      "capacity_fade_percent_per_year": 4.5
    }
  },
  "cycle_aging_model": {
    "description": "NCA cycle aging occurs through particle fracture and loss of electrical contact. Fast charge rates accelerate degradation.",
    "equation": "Q_loss_cyc = alpha_cyc * N^beta_cyc * exp(-E_a_cyc / (R*T)) * DOD^gamma_cyc",
    "parameters": {
      "alpha_cyc": {
        "value": 0.03,
        "unit": "dimensionless",
        "description": "Cycle aging pre-exponential factor"
      },
      "beta_cyc": {
        "value": 0.58,
        "unit": "dimensionless",
        "description": "Cycle count dependency exponent"
      },
      "E_a_cyc": {
        "value": 0.42,
        "unit": "eV",
        "description": "Cycle aging activation energy"
      },
      "gamma_cyc": {
        "value": 0.95,
        "unit": "dimensionless",
        "description": "DOD dependency exponent"
      }
    },
    "reference_conditions": {
      "temperature_C": 25,
      "DOD": 0.8,
      "C_rate": 0.5,
      "cycles_to_80_percent_SOH": 1500
    }
  },
