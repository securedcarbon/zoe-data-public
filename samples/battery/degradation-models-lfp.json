  "$schema": "../../schemas/battery/degradation-model.schema.json",
  "metadata": {
    "chemistry": "LFP-Graphite",
    "chemistry_full_name": "Lithium Iron Phosphate - Graphite",
    "data_source": "NREL BLAST-Lite",
    "source_url": "https://github.com/NREL/BLAST-Lite",
    "model_version": "2024.1",
    "last_updated": "2025-11-29",
    "validation_status": "validated",
    "confidence_level": "high",
    "notes": "Primary model based on NREL BLAST-Lite with validation from peer-reviewed literature. Suitable for grid-scale BESS applications."
  },
  "chemistry_characteristics": {
    "energy_density_wh_kg": 120,
    "power_density_w_kg": 300,
    "nominal_voltage": 3.2,
    "typical_cycle_life": 4000,
    "calendar_life_years": 15,
    "safety_rating": "excellent",
    "cost_relative": "low",
    "temperature_sensitivity": "moderate"
  },
  "calendar_aging_model": {
    "description": "Calendar aging reduces capacity over time due to SEI layer growth and electrolyte decomposition, primarily driven by temperature and SOC.",
    "equation": "Q_loss_cal = alpha_cal * t^beta_cal * exp(-E_a_cal / (R*T)) * SOC^gamma_cal",
    "parameters": {
      "alpha_cal": {
        "value": 0.025,
        "unit": "dimensionless",
        "description": "Calendar aging pre-exponential factor"
      },
      "beta_cal": {
        "value": 0.5,
        "unit": "dimensionless",
        "description": "Time dependency exponent (square root of time)"
      },
      "E_a_cal": {
        "value": 0.45,
        "unit": "eV",
        "description": "Calendar aging activation energy"
      },
      "gamma_cal": {
        "value": 0.3,
        "unit": "dimensionless",
        "description": "SOC dependency exponent"
      },
      "R": {
        "value": 8.617e-5,
        "unit": "eV/K",
        "description": "Boltzmann constant"
      }
    },
    "reference_conditions": {
      "temperature_C": 25,
      "SOC": 0.5,
      "capacity_fade_percent_per_year": 2.5
    }
  },
  "cycle_aging_model": {
    "description": "Cycle aging causes capacity fade through electrode particle cracking, lithium plating, and active material loss, dependent on DOD, temperature, and C-rate.",
    "equation": "Q_loss_cyc = alpha_cyc * N^beta_cyc * exp(-E_a_cyc / (R*T)) * DOD^gamma_cyc",
    "parameters": {
      "alpha_cyc": {
        "value": 0.015,
        "unit": "dimensionless",
        "description": "Cycle aging pre-exponential factor"
      },
      "beta_cyc": {
        "value": 0.52,
        "unit": "dimensionless",
        "description": "Cycle count dependency exponent"
      },
      "E_a_cyc": {
        "value": 0.35,
        "unit": "eV",
        "description": "Cycle aging activation energy"
      },
      "gamma_cyc": {
        "value": 0.8,
        "unit": "dimensionless",
        "description": "DOD dependency exponent"
      }
    },
    "reference_conditions": {
      "temperature_C": 25,
      "DOD": 0.8,
      "C_rate": 0.5,
      "cycles_to_80_percent_SOH": 4000
    }
  },
  "operating_conditions": {
    "temperature_range": {
      "min_C": -20,
      "optimal_C": 25,
      "max_C": 60,
      "notes": "Performance degrades rapidly below 0°C and above 45°C"
